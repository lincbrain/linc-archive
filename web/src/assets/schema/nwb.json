{
  "$schema": "http://json-schema.org/draft-07/schema",
  "type": "object",
  "title": "NWB",
  "required": [
    "sex",
    "organism",
    "number_subjects"
  ],
  "properties": {
    "measurement_type": {
      "$id": "#/properties/measurement_type",
      "type": "string",
      "title": "Measurement Type"
    },
    "variable_measured": {
      "$id": "#/properties/variable_measured",
      "type": "array",
      "title": "Variable Measured"
    },
    "age": {
      "$id": "#/properties/age",
      "type": "object",
      "title": "Age",
      "required": [
        "minimum",
        "maximum",
        "units"
      ],
      "properties": {
        "minimum": {
          "$id": "#/properties/age/properties/minimum",
          "type": "number",
          "title": "Minimum"
        },
        "maximum": {
          "$id": "#/properties/age/properties/maximum",
          "type": "number",
          "title": "Maximum"
        },
        "units": {
          "$id": "#/properties/age/properties/units",
          "type": "string",
          "title": "Units"
        },
        "categorical": {
          "$id": "#/properties/age/properties/categorical",
          "type": "object",
          "title": "Categorical"
        }
      }
    },
    "sex": {
      "$id": "#/properties/sex",
      "type": "array",
      "title": "Sex",
      "items": {
        "$id": "#/properties/sex/items",
        "type": "string",
        "title": "Items"
      }
    },
    "organism": {
      "$id": "#/properties/organism",
      "type": "array",
      "title": "Organism",
      "items": {
        "$id": "#/properties/organism/items",
        "type": "object",
        "title": "Items",
        "required": [
          "species"
        ],
        "properties": {
          "species": {
            "$id": "#/properties/organism/items/properties/species",
            "type": "string",
            "title": "Species"
          },
          "strain": {
            "$id": "#/properties/organism/items/properties/strain",
            "type": "string",
            "title": "Strain"
          },
          "identifier": {
            "$id": "#/properties/organism/items/properties/identifier",
            "type": "string",
            "title": "Identifier"
          },
          "vendor": {
            "$id": "#/properties/organism/items/properties/vendor",
            "type": "string",
            "title": "Vendor"
          }
        }
      }
    },
    "number_subjects": {
      "$id": "#/properties/number_subjects",
      "type": "integer",
      "title": "Number of Subjects"
    },
    "number_tissueSamples": {
      "$id": "#/properties/number_tissueSamples",
      "type": "integer",
      "title": "Number of Tissue Samples"
    },
    "number_cells": {
      "$id": "#/properties/number_cells",
      "type": "integer",
      "title": "Number of Cells "
    }
  }
}
